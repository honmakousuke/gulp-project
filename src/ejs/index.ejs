                            <li><img src="assets/images/npm.svg" alt="npm" title="npm"></li>
                            <li><img src="assets/images/sass.svg" alt="sass" title="sass"></li>
                            <li><img src="assets/images/gulp.svg" alt="gulp" title="gulp"></li>
                            <li><img src="assets/images/wordpress.svg" alt="wordpress" title="wordpress"></li>
                            <li><img src="assets/images/laravel.svg" alt="laravel" title="laravel"></li>
                            <li><img src="assets/images/vagrant.svg" alt="vagrant" title="vagrant"></li> 
                            <li><img src="assets/images/bitbucket.svg" alt="bitbucket" title="bitbucket"></li>
